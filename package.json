{
  "name": "stock-portfolio",
  "version": "0.1.0",
  "description": "Fullstack web-based stock portfolio app",
  "private": true,
  "author": "Grace Huang",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "connect-session-sequelize": "^6.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "history": "^4.10.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.4",
    "socket.io": "^2.3.0"
  },
  "scripts": {
    "build": "cd client/ && npm install && npm run build",
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run start-server & NODE_ENV='development' cd client/ && npm start",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "npm-merge-driver": "^2.3.5"
  }
}
